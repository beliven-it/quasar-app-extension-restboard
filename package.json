{
  "name": "@beliven/quasar-app-extension-restboard",
  "version": "0.54.0",
  "description": "A starter kit to install Restboard in a fresh new Quasar project",
  "author": "Emanuele Bertoldi <emanuele.bertoldi@gmail.com>",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "docs:gen:layouts": "npx vuelentino -i src/templates/core/src/layouts/**/*.vue  -o docs/layouts",
    "docs:gen:pages": "npx vuelentino -i src/templates/core/src/pages/**/*.vue  -o docs/pages",
    "docs:gen": "npm run docs:gen:layouts && npm run docs:gen:pages",
    "test": "echo \"No test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beliven-it/quasar-app-extension-restboard"
  },
  "bugs": "https://github.com/beliven-it/quasar-app-extension-restboard/issues",
  "homepage": "https://github.com/beliven-it/quasar-app-extension-restboard",
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "devDependencies": {
    "commander": "^11.1.0",
    "@beliven/quasar-app-extension-rb-ui": "^0.49.0",
    "vuelentino": "^0.0.4"
  },
  "dependencies": {
    "@beliven/rb-auth-provider-simple": "^0.23.0",
    "@beliven/rb-core-module": "^0.32.0",
    "@beliven/rb-data-provider-json-server": "^0.33.0"
  },
  "bin": "./cli/index.js",
  "publishConfig": {
    "access": "public"
  }
}
